{
    "version": 2,
    "functions": {
        "pages/api/**/*.js": {
            "maxDuration": 60
        }
    },
    "builds": [
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/pages/api/$1"
        }
    ]
}